###  http://www.example.com/ns/dsi#hasCoverageFactor
dsi:hasCoverageFactor rdf:type owl:DatatypeProperty ;
                      rdfs:range xsd:double .


###  http://www.example.com/ns/dsi#hasCoverageProbability
dsi:hasCoverageProbability rdf:type owl:DatatypeProperty ;
                           rdfs:range xsd:double .


###  http://www.example.com/ns/dsi#hasIntervalMax
dsi:hasIntervalMax rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:double .


###  http://www.example.com/ns/dsi#hasIntervalMin
dsi:hasIntervalMin rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:double .


###  http://www.example.com/ns/dsi#hasStandardUncertainty
dsi:hasStandardUncertainty rdf:type owl:DatatypeProperty ;
                           rdfs:range xsd:float .


###  http://www.example.com/ns/qod#CriticalIf
:CriticalIf rdf:type owl:DatatypeProperty ;
            rdfs:domain :AccuracyInterpretation ,
                        :BatteryInterpretation ,
                        :CompletenessInterpretation ,
                        :MeasurementUncertaintyInterpretation ,
                        :RecencyInterpretation ,
                        :TimelinessInterpretation ;
            rdfs:range [ rdf:type rdfs:Datatype ;
                         owl:unionOf ( <http://org.semanticweb.owlapi/error#Error3>
                                       <http://org.semanticweb.owlapi/error#Error4>
                                       rdf:XMLLiteral
                                     )
                       ] ;
            owl:propertyDisjointWith :SufficientIf .


